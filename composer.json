{
    "name": "nevadskiy/nova-icon-picker",
    "description": "Icon Picker for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "icon",
        "picker"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Nevadskiy\\Nova\\IconPicker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nevadskiy\\Nova\\IconPicker\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
